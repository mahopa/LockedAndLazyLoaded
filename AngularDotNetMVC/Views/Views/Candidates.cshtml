
@{
    Layout = null;
}
<h1>Candidates</h1>
<div class="container">
    <div class="row">
        <div ng-repeat="candidate in Candidates" class="col-md-6">
            <div class="row">
                <div class="col-md-6">
                    <h2>{{candidate.name}}</h2>
                    <h3>{{candidate.tagline}}</h3>
                </div>
                <div class="col-md-6"><img class="img-responsive" ng-src="{{candidate.image}}" /></div>
            </div class="row">
        </div>
    </div>

</div>

